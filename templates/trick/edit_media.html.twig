{% extends 'base.html.twig' %}

{% block title %}{{ 'page.edit.media.title'|trans }}{{ trick.name }}{% endblock %}

{% form_theme form '/partials/_collectionEditMedia.html.twig' %}


{% block body %}
    <div class="container">
        <h1>{{ 'form.trick.add.title'|trans }}</h1>
        {{ form_start(form) }}
        {{ form_widget(form) }}
        <button type="submit" class="btn btn-success">{{ 'form.trick.button.add'|trans }}</button>
        {{ form_end(form) }}
    </div>
{% endblock %}

{% block javascripts %}
    {% include '/partials/_js.html.twig' %}
{% endblock %}
