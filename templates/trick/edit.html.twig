{% extends 'base.html.twig' %}

{% block title %}{{ 'form.trick.edit.title'|trans }}{% endblock %}

{% form_theme form '/partials/_collectionMedia.html.twig' %}

{% block body %}
    <div class="container">
        <h1>{{ 'form.trick.edit.title'|trans }} {{ 'word.punctuation.cot'|trans }}{{ trick.name }}{{ 'word.punctuation.cot'|trans }}</h1>
        {{ form_start(form) }}
        {{ form_widget(form) }}
        <button type="submit" class="btn btn-primary">{{ 'form.trick.button.edit'|trans }}</button>
        {{ form_end(form) }}
    </div>
{% endblock %}


