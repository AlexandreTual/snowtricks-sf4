{% extends 'base.html.twig' %}

{% block title %}{{ 'page.registration.title'|trans }}{% endblock %}

{% block body %}
    <div class="container">
        <h1>{{ 'form.registration.title'|trans }}</h1>
        {{ form_start(form) }}
        {{ form_widget(form) }}
        <button type="submit" class="btn btn-primary">{{ 'form.registration.submit'|trans }}</button>
        {{ form_end(form) }}
    </div>
{% endblock %}